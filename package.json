    "dev": "tsx src/index.ts",
    "dev:watch": "tsx watch src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "extract": "NODE_ENV=production tsx src/index.ts",
    "format": "prettier --write \"src/**/*.ts\""
