    "graphql": "^16.12.0",
    "graphql-request": "^7.4.0",
    "pino": "^10.1.0",
    "starknet": "^9.2.1",
    "viem": "^2.44.2",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@types/pino": "^7.0.4",
    "eslint": "^9.39.2",
    "pino-pretty": "^13.1.3",
    "prettier": "^3.7.4",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
